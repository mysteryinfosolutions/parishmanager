#
# TABLE STRUCTURE FOR: mdioceses
#

DROP TABLE IF EXISTS `mdioceses`;

CREATE TABLE `mdioceses` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `established_in` varchar(255) NOT NULL,
  `contact1` varchar(255) NOT NULL,
  `contact2` varchar(255) NOT NULL,
  `email1` varchar(255) NOT NULL,
  `email2` varchar(255) NOT NULL,
  `website` varchar(255) NOT NULL,
  `addressline1` varchar(255) NOT NULL,
  `addressline2` varchar(255) NOT NULL,
  `city` varchar(255) NOT NULL,
  `mstate_id` int(11) NOT NULL,
  `pincode` varchar(255) NOT NULL,
  `timage_id` int(11) NOT NULL DEFAULT '1',
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

INSERT INTO `mdioceses` (`id`, `name`, `established_in`, `contact1`, `contact2`, `email1`, `email2`, `website`, `addressline1`, `addressline2`, `city`, `mstate_id`, `pincode`, `timage_id`, `created_at`, `updated_at`, `deleted_at`, `status`) VALUES (1, 'Roman Catholic Diocese of Darjeeling', '1962', '0354 2254027', '9434007041', 'bishoplepcha@gmail.com', '', 'http://www.diocesedarjeeling.com', 'Bishop\'s House', '13, Dr. Yensing Road', 'Darjeeling', 1, '734101', 1, '2018-12-12 00:52:27', '2019-02-20 18:06:10', NULL, 1);


#
# TABLE STRUCTURE FOR: mevents
#

DROP TABLE IF EXISTS `mevents`;

CREATE TABLE `mevents` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8;

