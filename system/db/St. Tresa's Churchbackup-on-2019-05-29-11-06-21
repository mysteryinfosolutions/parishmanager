#
# TABLE STRUCTURE FOR: tsisters
#

DROP TABLE IF EXISTS `tsisters`;

CREATE TABLE `tsisters` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `tsister_id` bigint(15) NOT NULL,
  `mconvent_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `superior_flag` tinyint(4) NOT NULL DEFAULT '0',
  `status` tinyint(4) NOT NULL DEFAULT '0',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`tsister_id`),
  KEY `id` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

#
# TABLE STRUCTURE FOR: ttables
#

DROP TABLE IF EXISTS `ttables`;

CREATE TABLE `ttables` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `primary_key` varchar(255) NOT NULL,
  `priority` tinyint(4) NOT NULL DEFAULT '5',
  `sync` tinyint(4) NOT NULL DEFAULT '1',
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=22 DEFAULT CHARSET=utf8;

