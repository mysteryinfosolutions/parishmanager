#
# TABLE STRUCTURE FOR: ttransactions
#

DROP TABLE IF EXISTS `ttransactions`;

CREATE TABLE `ttransactions` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `ttransaction_id` varchar(255) NOT NULL,
  `voucher_number` varchar(255) NOT NULL,
  `cash_debit` decimal(19,2) DEFAULT NULL,
  `cash_credit` decimal(19,2) DEFAULT NULL,
  `mbankaccount_id` int(11) DEFAULT NULL,
  `bank_credit` decimal(19,2) DEFAULT NULL,
  `bank_debit` decimal(19,2) DEFAULT NULL,
  `fixed_deposit` tinyint(4) NOT NULL DEFAULT '0',
  `date` varchar(255) DEFAULT NULL,
  `mfinancialyear_id` int(11) DEFAULT NULL,
  `mledger_id` int(11) DEFAULT NULL,
  `tledger_id` int(11) DEFAULT NULL,
  `mparish_id` int(11) DEFAULT NULL,
  `mscc_id` int(11) DEFAULT NULL,
  `mfamily_id` int(11) DEFAULT NULL,
  `remarks` text,
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `status` tinyint(4) DEFAULT '0',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`ttransaction_id`),
  KEY `id` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

INSERT INTO `ttransactions` (`id`, `ttransaction_id`, `voucher_number`, `cash_debit`, `cash_credit`, `mbankaccount_id`, `bank_credit`, `bank_debit`, `fixed_deposit`, `date`, `mfinancialyear_id`, `mledger_id`, `tledger_id`, `mparish_id`, `mscc_id`, `mfamily_id`, `remarks`, `created_at`, `updated_at`, `deleted_at`, `status`, `sync_flag`) VALUES (2, 'P10010021', 'P/1/2019-20', NULL, '679.00', NULL, NULL, NULL, 0, '2019-04-03', 1001002, 31, NULL, 1001, NULL, NULL, '', '2019-04-03 16:54:01', '2019-04-03 16:54:01', NULL, 1, 1);
INSERT INTO `ttransactions` (`id`, `ttransaction_id`, `voucher_number`, `cash_debit`, `cash_credit`, `mbankaccount_id`, `bank_credit`, `bank_debit`, `fixed_deposit`, `date`, `mfinancialyear_id`, `mledger_id`, `tledger_id`, `mparish_id`, `mscc_id`, `mfamily_id`, `remarks`, `created_at`, `updated_at`, `deleted_at`, `status`, `sync_flag`) VALUES (1, 'R10010021', 'R/1/2019-20', '2000.00', NULL, NULL, NULL, NULL, 0, '2019-04-03', 1001002, 15, NULL, 1001, NULL, NULL, '', '2019-04-03 16:53:37', '2019-04-03 16:53:37', NULL, 1, 1);


