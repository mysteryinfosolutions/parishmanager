#
# TABLE STRUCTURE FOR: mfamilies
#

DROP TABLE IF EXISTS `mfamilies`;

CREATE TABLE `mfamilies` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mfamily_id` int(11) NOT NULL,
  `firstname` varchar(255) NOT NULL,
  `lastname` varchar(255) NOT NULL,
  `addressline1` varchar(255) NOT NULL,
  `addressline2` varchar(255) NOT NULL,
  `city` varchar(255) NOT NULL,
  `mstate_id` int(11) NOT NULL,
  `pincode` varchar(255) NOT NULL,
  `mscc_id` int(11) NOT NULL,
  `mparish_id` int(11) NOT NULL,
  `remarks` text NOT NULL,
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '0',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`mfamily_id`) USING BTREE,
  KEY `id` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=266 DEFAULT CHARSET=utf8;

