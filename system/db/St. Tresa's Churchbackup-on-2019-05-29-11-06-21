INSERT INTO `mupdater` (`id`, `filename`, `content`, `version`, `path`, `created_at`, `updated_at`, `deleted_at`, `update_flag`, `status`) VALUES (12, 'Myencrypt.php', '&lt;?php\n\ndefined(\'BASEPATH\') OR exit(\'No direct script access allowed\');\n\nclass Myencrypt {\n\n    function encrypt_url($string) {\n\n        $output = false;\n\n        /*\n\n         * read security.ini file &amp; get encryption_key | iv | encryption_mechanism value for generating encryption code\n\n         */\n\n\n\n        $security = parse_ini_file(\'security.ini\');\n\n        $secret_key = $security[\'encryption_key\'];\n\n        $secret_iv = $security[\'iv\'];\n\n        $encrypt_method = $security[\'encryption_mechanism\'];\n\n\n\n        // hash\n\n        $key = hash(\'sha256\', $secret_key);\n\n\n\n        // iv &ndash; encrypt method AES-256-CBC expects 16 bytes &ndash; else you will get a warning\n\n        $iv = substr(hash(\'sha256\', $secret_iv), 0, 16);\n\n\n\n        //do the encryption given text/string/number\n\n\n\n        $result = openssl_encrypt($string, $encrypt_method, $key, 0, $iv);\n\n        $output = base64_encode($result);\n\n\n\n        return $output;\n    }\n\n    function decrypt_url($string) {\n\n        $output = false;\n\n\n\n        /*\n\n         * read security.ini file &amp; get encryption_key | iv | encryption_mechanism value for generating encryption code\n\n         */\n\n\n\n        $security = parse_ini_file(\'security.ini\');\n\n        $secret_key = $security[\'encryption_key\'];\n\n        $secret_iv = $security[\'iv\'];\n\n        $encrypt_method = $security[\'encryption_mechanism\'];\n\n\n\n// hash\n\n        $key = hash(\'sha256\', $secret_key);\n\n\n\n        // iv &ndash; encrypt method AES-256-CBC expects 16 bytes &ndash; else you will get a warning\n\n        $iv = substr(hash(\'sha256\', $secret_iv), 0, 16);\n\n\n\n        //do the decryption given text/string/number\n\n\n\n        $output = openssl_decrypt(base64_decode($string), $encrypt_method, $key, 0, $iv);\n\n        return $output;\n    }\n\n}\n', 1, './application/libraries/', '2019-03-24 12:07:24', '2019-03-24 12:07:24', NULL, 0, 1);
