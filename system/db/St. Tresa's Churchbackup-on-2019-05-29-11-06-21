#
# TABLE STRUCTURE FOR: mlogins
#

DROP TABLE IF EXISTS `mlogins`;

CREATE TABLE `mlogins` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mlogin_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `username` varchar(255) NOT NULL,
  `password` varchar(255) NOT NULL,
  `mrole_id` int(11) NOT NULL,
  `maccessright_id` int(11) NOT NULL,
  `target_id` int(11) NOT NULL,
  `mtheme_id` int(11) NOT NULL DEFAULT '5',
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`mlogin_id`),
  KEY `id` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

