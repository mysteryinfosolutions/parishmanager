#
# TABLE STRUCTURE FOR: mmembers
#

DROP TABLE IF EXISTS `mmembers`;

CREATE TABLE `mmembers` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mmember_id` int(11) NOT NULL,
  `membersince` varchar(255) NOT NULL,
  `firstname` varchar(255) NOT NULL,
  `lastname` varchar(255) NOT NULL,
  `mgender_id` int(11) NOT NULL,
  `dateofbirth` varchar(255) NOT NULL,
  `father` varchar(255) NOT NULL,
  `mother` varchar(255) NOT NULL,
  `mqualification_id` int(11) NOT NULL,
  `moccupation_id` int(11) NOT NULL,
  `contact1` varchar(255) NOT NULL,
  `contact2` varchar(255) NOT NULL,
  `email1` varchar(255) NOT NULL,
  `addressline1` varchar(255) NOT NULL,
  `addressline2` varchar(255) NOT NULL,
  `city` varchar(255) NOT NULL,
  `mstate_id` int(11) NOT NULL,
  `pincode` varchar(255) NOT NULL,
  `mfamily_id` int(11) NOT NULL,
  `mscc_id` int(11) NOT NULL,
  `mparish_id` int(11) NOT NULL,
  `baptism_flag` tinyint(4) NOT NULL DEFAULT '0',
  `communion_flag` tinyint(4) NOT NULL DEFAULT '0',
  `confirmation_flag` tinyint(4) NOT NULL DEFAULT '0',
  `marriage_flag` tinyint(4) NOT NULL DEFAULT '0',
  `vocation_flag` tinyint(4) NOT NULL DEFAULT '0',
  `death_flag` tinyint(4) NOT NULL DEFAULT '0',
  `remarks` text NOT NULL,
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `active` tinyint(4) NOT NULL DEFAULT '0',
  `status` tinyint(4) NOT NULL DEFAULT '0',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`mmember_id`) USING BTREE,
  KEY `id` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=804 DEFAULT CHARSET=utf8;

