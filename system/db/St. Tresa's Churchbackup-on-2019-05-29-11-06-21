#
# TABLE STRUCTURE FOR: mabouts
#

DROP TABLE IF EXISTS `mabouts`;

CREATE TABLE `mabouts` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `property` varchar(255) NOT NULL,
  `name` varchar(255) NOT NULL,
  `data` text NOT NULL,
  `remarks` text NOT NULL,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

INSERT INTO `mabouts` (`id`, `property`, `name`, `data`, `remarks`, `updated_at`, `status`) VALUES (1, 'dbversion', 'Database Version', '0.0.2', '', '2019-03-12 09:12:07', 1);
INSERT INTO `mabouts` (`id`, `property`, `name`, `data`, `remarks`, `updated_at`, `status`) VALUES (2, 'appversion', 'Application Version', '1.00237', '', '2019-03-20 10:51:54', 1);


#
# TABLE STRUCTURE FOR: maccessrights
#

DROP TABLE IF EXISTS `maccessrights`;

CREATE TABLE `maccessrights` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8;

INSERT INTO `maccessrights` (`id`, `name`, `status`) VALUES (1, 'Full Control', 1);
INSERT INTO `maccessrights` (`id`, `name`, `status`) VALUES (2, 'Partial Control', 1);
INSERT INTO `maccessrights` (`id`, `name`, `status`) VALUES (3, 'Minimal Control', 1);


#
# TABLE STRUCTURE FOR: maccountindex
#

DROP TABLE IF EXISTS `maccountindex`;

CREATE TABLE `maccountindex` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mparish_id` int(11) NOT NULL,
  `financialyear_id` int(11) NOT NULL DEFAULT '0',
  `bankaccount_id` int(11) NOT NULL DEFAULT '0',
  `ledger_id` int(11) NOT NULL DEFAULT '0',
  `status` tinyint(4) NOT NULL DEFAULT '1',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`mparish_id`),
  KEY `id` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

INSERT INTO `maccountindex` (`id`, `mparish_id`, `financialyear_id`, `bankaccount_id`, `ledger_id`, `status`, `sync_flag`) VALUES (1, 1001, 2, 0, 8, 1, 0);


#
# TABLE STRUCTURE FOR: massociations
#

DROP TABLE IF EXISTS `massociations`;

CREATE TABLE `massociations` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `abbreviation` varchar(255) NOT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

