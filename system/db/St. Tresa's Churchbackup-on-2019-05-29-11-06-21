  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mscc_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `sakha` varchar(255) NOT NULL,
  `president` varchar(255) NOT NULL,
  `president_contact` varchar(255) NOT NULL,
