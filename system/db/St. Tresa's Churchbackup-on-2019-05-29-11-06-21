#
# TABLE STRUCTURE FOR: mparishes
#

DROP TABLE IF EXISTS `mparishes`;

CREATE TABLE `mparishes` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mparish_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `established_in` varchar(255) NOT NULL,
  `contact1` varchar(255) NOT NULL,
  `contact2` varchar(255) NOT NULL,
  `email1` varchar(255) NOT NULL,
  `addressline1` varchar(255) NOT NULL,
  `addressline2` varchar(255) NOT NULL,
  `city` varchar(255) NOT NULL,
  `mstate_id` int(11) NOT NULL,
  `pincode` varchar(255) NOT NULL,
  `mdeanary_id` int(11) NOT NULL,
  `mdiocese_id` int(11) NOT NULL,
  `deanparish_flag` tinyint(4) NOT NULL DEFAULT '0',
  `quasiparish_flag` tinyint(4) NOT NULL DEFAULT '0',
  `cathedral_flag` tinyint(4) NOT NULL DEFAULT '0',
  `remarks` text NOT NULL,
  `activeparish` tinyint(4) NOT NULL DEFAULT '0',
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `timage_id` int(11) NOT NULL DEFAULT '3',
  `status` tinyint(4) NOT NULL DEFAULT '0',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`mparish_id`),
  KEY `id` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=23 DEFAULT CHARSET=utf8;

