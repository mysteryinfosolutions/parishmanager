#
# TABLE STRUCTURE FOR: mbankaccounts
#

DROP TABLE IF EXISTS `mbankaccounts`;

CREATE TABLE `mbankaccounts` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mbankaccount_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `bank_name` varchar(255) NOT NULL,
  `branch` varchar(255) NOT NULL,
  `account_number` varchar(255) NOT NULL,
  `ifsc_code` varchar(255) NOT NULL,
  `remarks` text NOT NULL,
  `mparish_id` int(11) NOT NULL,
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '0',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`mbankaccount_id`),
  KEY `id` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

#
# TABLE STRUCTURE FOR: mcatechists
#

DROP TABLE IF EXISTS `mcatechists`;

CREATE TABLE `mcatechists` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mcatechist_id` int(11) NOT NULL,
  `mmember_id` int(11) NOT NULL,
  `mparish_id` int(11) NOT NULL,
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '1',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`mcatechist_id`),
  KEY `id` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

#
# TABLE STRUCTURE FOR: mcongregations
#

DROP TABLE IF EXISTS `mcongregations`;

CREATE TABLE `mcongregations` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

INSERT INTO `mcongregations` (`id`, `name`, `status`) VALUES (1, 'IBVM', 1);
INSERT INTO `mcongregations` (`id`, `name`, `status`) VALUES (2, 'Cluny', 1);


#
# TABLE STRUCTURE FOR: mconvents
#

DROP TABLE IF EXISTS `mconvents`;

CREATE TABLE `mconvents` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mconvent_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `mcongregation_id` int(11) NOT NULL,
  `addressline1` varchar(255) NOT NULL,
  `addressline2` varchar(255) NOT NULL,
  `city` varchar(255) NOT NULL,
  `mstate_id` int(11) NOT NULL,
  `pincode` varchar(255) NOT NULL,
  `mparish_id` int(11) NOT NULL,
  `remarks` text NOT NULL,
  `member_id` int(11) NOT NULL DEFAULT '0',
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '0',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`mconvent_id`),
  KEY `id` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

#
# TABLE STRUCTURE FOR: mcountries
#

DROP TABLE IF EXISTS `mcountries`;

CREATE TABLE `mcountries` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `code` varchar(255) NOT NULL DEFAULT '--',
  `created_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `updated_at` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `deleted_at` datetime DEFAULT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=244 DEFAULT CHARSET=utf8;

