#
# TABLE STRUCTURE FOR: mparishindex
#

DROP TABLE IF EXISTS `mparishindex`;

CREATE TABLE `mparishindex` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `mparish_id` int(11) NOT NULL DEFAULT '0',
  `scc_id` int(11) NOT NULL DEFAULT '0',
  `family_id` int(11) NOT NULL DEFAULT '0',
  `member_id` int(11) NOT NULL DEFAULT '0',
  `substation_id` int(11) NOT NULL DEFAULT '0',
  `institution_id` int(11) NOT NULL DEFAULT '0',
  `convent_id` int(11) NOT NULL DEFAULT '0',
  `association_id` int(11) NOT NULL DEFAULT '0',
  `sister_id` int(11) NOT NULL DEFAULT '0',
  `catechist_id` int(11) NOT NULL DEFAULT '0',
  `status` tinyint(4) NOT NULL DEFAULT '1',
  `sync_flag` tinyint(4) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

INSERT INTO `mparishindex` (`id`, `mparish_id`, `scc_id`, `family_id`, `member_id`, `substation_id`, `institution_id`, `convent_id`, `association_id`, `sister_id`, `catechist_id`, `status`, `sync_flag`) VALUES (1, 1001, 42, 265, 803, 0, 0, 0, 0, 0, 0, 1, 0);


#
# TABLE STRUCTURE FOR: mparishtypes
#

DROP TABLE IF EXISTS `mparishtypes`;

CREATE TABLE `mparishtypes` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `status` int(11) NOT NULL DEFAULT '1',
  PRIMARY KEY (`id`)
